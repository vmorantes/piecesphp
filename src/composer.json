{
    "name": "piecesphp/piecesphp",
    "description": "Framework PHP",
    "require": {
        "php": "^8.1.0",
        "ext-date": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-hash": "*",
        "ext-session": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-mysqli": "*",
        "ext-pdo_mysql": "*",
        "ext-sqlite3": "*",
        "ext-pdo_sqlite": "*",
        "ext-zip": "*",
        "ext-xsl": "*",
        "ext-xmlwriter": "*",
        "ext-xmlreader": "*",
        "ext-curl": "*",
        "ifsnop/mysqldump-php": "^2.12",
        "mpdf/mpdf": "^8.2",
        "phpmailer/phpmailer": "6.*",
        "phpoffice/phpspreadsheet": "^1.29",
        "piecesphp/database": "^2.5",
        "piecesphp/datastructures": "~2",
        "piecesphp/geojson": "^1.1",
        "piecesphp/html": "~1.1",
        "scssphp/scssphp": "^1.12",
        "slim/psr7": "^1.6",
        "slim/slim": "4.*",
        "spatie/url": "^1.3",
        "studio-42/elfinder": "^2.1",
        "microsoft/azure-storage-blob": "^1.5",
        "pragmarx/google2fa": "^8.0",
        "hubspot/api-client": "^11.3",
        "openai-php/client": "^0.10.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4"
    },
    "suggest": {
        "ext-zlib": ">=1.2.0.4",
        "ext-mongodb": "^1.12.0",
        "mongodb/mongodb": "~1.11"
    },
    "autoload": {
        "psr-4": {
            "PiecesPHP\\ComposerTasks\\": "../tasks",
            "PiecesPHP\\Core\\": "./app/core/psr4/PiecesPHP/Core"
        }
    },
    "scripts": {
        "task": [
            "PiecesPHP\\ComposerTasks\\TasksManager::task"
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
